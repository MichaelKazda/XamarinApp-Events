<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EventsApp.Pages.SingUpPage">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="CenterAndExpand" 
                     VerticalOptions="CenterAndExpand">
            <Label Text="Just Log in!" FontSize="30" TextColor="black" FontAttributes="Bold" HorizontalOptions="Center"/>
            <Button Clicked="SingUpButton_ClickedAsync" Text="Sing Up with Google" HorizontalOptions="Center" Padding="15"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>